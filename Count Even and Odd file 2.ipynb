    "num = (1,2,3,4,5,6,7,8,9)\n",
    "count1 = 0\n",
    "count2 = 0\n",
    "for i in range(len(num)):\n",
    "    if(num[i]%2 == 0):\n",
    "        count1 += 1\n",
    "    else:\n",
    "        count2 += 1\n",
    "print('Number of even numbers : ',count1)\n",
    "print('Number of odd numbers : ',count2)"
